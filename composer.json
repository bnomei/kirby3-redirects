{
    "name": "bnomei/kirby3-redirects",
    "type": "kirby-plugin",
    "description": "Setup HTTP Status Code Redirects from within the Kirby Panel",
    "version": "1.2.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "redirect",
        "301",
        "302",
        "307",
        "308",
        "status-codes",
        "header",
        "redirects"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/composer-installer": "^1.1"
    }
}
